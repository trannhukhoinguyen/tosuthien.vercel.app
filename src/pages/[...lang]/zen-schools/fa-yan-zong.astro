---
import Base from "../../../layouts/Base.astro";
import { supportedLang } from "../../../lib/utils/languageParser";
import PeriodicTable from "../../../layouts/components/Chart/PeriodicTable.astro";
import sectDb from "../../../db/sectDb.tsx";
import { CnFayanMasters } from "../../../db/masters/AllMasterDb.tsx";

export function getStaticPaths() {
  const paths = supportedLang.map((lang) => ({
    params: { lang: lang || undefined },
  }));
  return paths;
}
const { lang } = Astro.params;
---

<Base>
  <PeriodicTable sect={ sectDb.find(sect=> sect.label === 'Fayan') } mastersDb={ CnFayanMasters } />
</Base>
