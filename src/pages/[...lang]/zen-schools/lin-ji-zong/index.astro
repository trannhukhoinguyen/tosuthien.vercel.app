---
import Base from "../../../../layouts/Base.astro";
import { supportedLang } from "../../../../lib/utils/languageParser";
import PeriodicTable from "../../../../layouts/components/PeriodicTable.astro";
import sectDb from "../../../../db/sectDb.tsx";
import { CnLinjiMasters } from "../../../../db/masters/AllMasterDb.tsx";
import MakeupPalettes from "../../../../layouts/components/MakeupPalettes.astro";

export function getStaticPaths() {
  const paths = supportedLang.map((lang) => ({
    params: { lang: lang || undefined },
  }));
  return paths;
}
const { lang } = Astro.params;
---

<Base>
<MakeupPalettes quantity={2} />
<PeriodicTable sect={ sectDb.find(sect=> sect.label === 'Linji') } mastersDb={ CnLinjiMasters } />
<hr/>

</Base>
